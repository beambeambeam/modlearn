{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./trpc": {
      "default": "./src/trpc/index.ts"
    },
    "./trpc/*": {
      "default": "./src/trpc/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/trpc": "^1.1.0",
    "@hono/trpc-server": "^0.4.0",
    "@modlearn/auth": "workspace:*",
    "@modlearn/db": "workspace:*",
    "@modlearn/env": "workspace:*",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "better-auth": "catalog:",
    "dotenv": "catalog:",
    "elysia": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@modlearn/config": "workspace:*",
    "@types/bun": "catalog:",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "aws-sdk-client-mock": "^4.1.0",
    "tsdown": "^0.16.5",
    "typescript": "catalog:",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.5"
  }
}
